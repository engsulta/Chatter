<AnchorPane fx:id="contactList" prefHeight="500.0" prefWidth="250.0" style="-fx-background-color: #DEDEDE;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jdevsul.list.ListController">
      
